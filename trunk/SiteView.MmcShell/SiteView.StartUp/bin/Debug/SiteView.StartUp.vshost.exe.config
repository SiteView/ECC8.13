<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section 
      name="SiteViewShell" 
      type="SiteView.StartUp.ShellConfigurationSectionHandler,SiteView.StartUp" />
  </configSections>
  <SiteViewShell 
    Url="http://localhost:8088/MmcPlayer/" 
    Local="C:\Program Files\SiteView\SnapIns" />
  <system.diagnostics>
    <sources>
      <source name="Executive" switchName="Executive"
        switchType="System.Diagnostics.SourceSwitch" >
        <listeners>
          <add name="console" />
          <remove name ="Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <!-- You can set the level at which tracing is to occur -->
      <add name="Executive" value="Information" />
      <!-- You can turn tracing off -->
      <!--add name="SourceSwitch" value="Off" -->
    </switches>
    <sharedListeners>
      <add name="console"
        type="System.Diagnostics.ConsoleTraceListener"
        initializeData="false"/>
    </sharedListeners>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="console" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>