<?xml version="1.0" encoding="UTF-8"?>

<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>SvEcc Component Services</description>
	<vendor>DragonFlow</vendor>

	<service name="getSiteSeerAttr" engine="java" 
		location="com.siteview.svecc.service.SiteSeer" invoke="getSiteSeerAttr">
		<attribute name="data" type="java.util.List" mode="OUT"
			optional="true" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>
	<service name="getSiteSeer" engine="java" 
		location="com.siteview.svecc.service.SiteSeer" invoke="getSiteSeer">
		<attribute name="data" type="java.util.Map" mode="OUT"
			optional="true" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>
	<service name="putSiteSeer" engine="java" 
		location="com.siteview.svecc.service.SiteSeer" invoke="putSiteSeer">
		<attribute name="data" type="java.util.Map" mode="IN"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

</services>