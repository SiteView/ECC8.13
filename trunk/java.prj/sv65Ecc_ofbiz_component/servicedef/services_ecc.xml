	<service name="getAllUser" engine="java" 
		location="com.siteview.svecc.service.User" invoke="getAllUser">
		<attribute name="result" type="java.util.List" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>
