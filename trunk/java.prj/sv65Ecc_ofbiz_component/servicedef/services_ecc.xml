<?xml version="1.0" encoding="UTF-8"?>

<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>SvEcc Component Services</description>
	<vendor>DragonFlow</vendor>

	<!-- Group Services define -->
  <!--
  功能：创建组
  输入：
    name - 组名
    parentId - 父组ID, 不填写的时候，为TOP组
    data - 组的属性 必须参照com.dragonflow.SiteView.Group 类的内容
  输出：
    message - 输出的错误信息，当无错误时，为新的组的id
    
  -->
	<service name="addGroup" engine="java" 
		location="com.siteview.svecc.service.Group" invoke="addGroup">
		<attribute name="name" type="String" mode="IN" optional="false" />
		<attribute name="parentId" type="String" mode="IN"
			optional="true" />
		<attribute name="data" type="java.util.Map" mode="IN"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="false" />
	</service>
  <!--
  功能：得到组的属性，及其缺省值
  输出：
    data - 组的属性 参照com.dragonflow.SiteView.Group 类的内容
    message - 输出的错误信息
    
  -->
  <service name="getGroupAttri" engine="java"
		location="com.siteview.svecc.service.Group"
		invoke="getGroupAttri" auth="false">
		<attribute name="data" mode="OUT" type="Map"
			optional="true">
		</attribute>
		<attribute name="message" mode="OUT" type="String"
			optional="true">
		</attribute>
	</service>

  <!--
  功能：删除组
  输入：
    id - 组id
  输出：
    message - 输出的错误信息
    
  -->
  <service name="delGroup" engine="java" 
		location="com.siteview.svecc.service.Group" invoke="delGroup">
		<attribute name="id" type="String" mode="IN" optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：得到组，通过组id
  输入：
    id - 组id
  输出：
    result - 组属性
    message - 输出的错误信息
    
  -->
  <service name="getGroupById" engine="java" 
		location="com.siteview.svecc.service.Group" invoke="getGroupById">
    <attribute name="id" type="String" mode="IN" optional="false" />
    <attribute name="result" type="java.util.Map" mode="OUT"
			optional="false" />
    <attribute name="message" type="String" mode="OUT"
			optional="true" />
  </service>

  <!--
  功能：通过组id，得到它的子组
  输入：
    parentId - 父组id
  输出：
    result - 组属性列表
    message - 输出的错误信息
    
  -->
  <service name="getGroupByParentId" engine="java" 
		location="com.siteview.svecc.service.Group"
		invoke="getGroupByParentId">
		<attribute name="parentId" type="String" mode="IN"
			optional="true" />
		<attribute name="result" type="java.util.List" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：得到所有的组
  输出：
    result - 组属性列表
    message - 输出的错误信息
    
  -->
  <service name="getAllGroup" engine="java" 
		location="com.siteview.svecc.service.Group" invoke="getAllGroup">
		<attribute name="result" type="java.util.List" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：更新组
  输入：
    id - 组id
    data - 组的属性 参照com.dragonflow.SiteView.Group 类的内容
  输出：
    message - 输出的错误信息
    
  -->
  <service name="updGroup" engine="java" 
		location="com.siteview.svecc.service.Group" invoke="updGroup">
		<attribute name="id" type="String" mode="IN" optional="false" />
		<attribute name="data" type="java.util.Map" mode="IN"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

	<!-- Monitor Services define -->
  <!--
  功能：创建监视器
  输入：
    className - 监视器类名，com\dragonflow\StandardMonitor下的类！带包名
    groupId - 组ID
    data - 监视器的属性 必须参照com.dragonflow.SiteView.Monitor 类的内容
  输出：
    message - 输出的错误信息，当无错误时，为新的监视器的id
    
  -->
  <service name="addMonitor" engine="java" 
		location="com.siteview.svecc.service.Monitor" invoke="addMonitor">
		<attribute name="className" type="String" mode="IN"
			optional="false" />
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="data" type="java.util.Map" mode="IN"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="false" />
	</service>
  <!--
  功能：得到监视器类型
  输入：
    keyword - 搜寻关键字
    bContains - 是否包含该关键字
    result - 搜寻结果列表
  输出：
    message - 输出的错误信息
    
  -->
  <service name="getMonitorType" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorType">
		<attribute name="keyword" type="String" mode="IN"
			optional="true" />
		<attribute name="bContains" type="Boolean" mode="IN"
			optional="false" default-value="true"/>
		<attribute name="result" type="java.util.List" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：得到监视器实例，通过类名
  输入：
    className - 类名称
    result - 监视器的属性 必须参照com.dragonflow.SiteView.Monitor 类的内容
  输出：
    message - 输出的错误信息
    
  -->
    <service name="getMonitorInstanceByClassName" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorInstanceByClassName">
		<attribute name="className" type="String" mode="IN"
			optional="false" />
		<attribute name="result" type="java.util.Map" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：得到监视器的可编辑属性
  输入：
    className - 类名称
    groupId - 组id
    id - 监视器id
    result - 监视器的属性 必须参照com.dragonflow.SiteView.Monitor 类的内容
  输出：
    message - 输出的错误信息
    
  -->
  <service name="getMonitorEditProperties" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorEditProperties">
		<attribute name="className" type="String" mode="IN"
			optional="true" />
		<attribute name="groupId" type="String" mode="IN"
			optional="true" />
		<attribute name="id" type="String" mode="IN"
			optional="true" />
		<attribute name="result" type="java.util.Map" mode="OUT"
			optional="true" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：删除监视器
  输入：
    groupId - 组id
    id - 监视器id
  输出：
    message - 输出的错误信息
    
  -->
  <service name="delMonitor" engine="java" 
		location="com.siteview.svecc.service.Monitor" invoke="delMonitor">
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="id" type="String" mode="IN" optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：得到监视器，通过id
  输入：
    groupId - 组id
    id - 监视器id
  输出：
    result - 监视器的属性 必须参照com.dragonflow.SiteView.Monitor 类的内容
    message - 输出的错误信息
  -->
   <service name="getMonitorById" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorById">
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="id" type="String" mode="IN" optional="false" />
		<attribute name="result" type="java.util.Map" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>
  <!--
  功能：得到所有监视器，通过组id
  输入：
    groupId - 组id
  输出：
    result - 监视器列表。其属性， 必须参照com.dragonflow.SiteView.Monitor 类的内容
    message - 输出的错误信息
  -->
  <service name="getMonitorByGroupId" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorByGroupId">
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="result" type="java.util.List" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

  <!--
  功能：得到所有监视器的动态信息(状态等。。。)，通过id
  输入：
    groupId - 组id
    id - 监视器id
  输出：
    result - 监视器的动态信息 必须参照com.dragonflow.SiteView.Monitor 类的内容
    message - 输出的错误信息
  -->
  <service name="getMonitorDYNInfoById" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorDYNInfoById">
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="id" type="String" mode="IN" optional="false" />
		<attribute name="result" type="java.util.Map" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>
  <!--
  功能：得到所有监视器的动态信息(状态等。。。)，通过组id
  输入：
    groupId - 组id
  输出：
    result - 监视器的动态信息列表。 必须参照com.dragonflow.SiteView.Monitor 类的内容
    message - 输出的错误信息
  -->
  <service name="getMonitorDYNInfoByGroupId" engine="java" 
		location="com.siteview.svecc.service.Monitor"
		invoke="getMonitorDYNInfoByGroupId">
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="result" type="java.util.List" mode="OUT"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>
  <!--
  功能：更新监视器信息(状态等。。。)，必须参照com.dragonflow.SiteView.Monitor 类的内容
  输入：
    groupId - 组id
    id - 监视器id
    data - 更新后的监视器信息
  输出：
    message - 输出的错误信息
  -->
	<service name="updMonitor" engine="java" 
		location="com.siteview.svecc.service.Monitor" invoke="updMonitor">
		<attribute name="groupId" type="String" mode="IN"
			optional="false" />
		<attribute name="id" type="String" mode="IN" optional="false" />
		<attribute name="data" type="java.util.Map" mode="IN"
			optional="false" />
		<attribute name="message" type="String" mode="OUT"
			optional="true" />
	</service>

	<!-- User Services define -->
  <!--
  功能：添加一个用户，必须参照必须参照com.siteview.svecc.service.User类的内容
